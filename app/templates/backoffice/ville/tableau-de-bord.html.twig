{% extends "base.html.twig" %}

{% block title %}{{ parent() }} - Votre demande préalable de travaux{% endblock %}

{% block content %}
    <main>
        <section class="section-color">
            <p class="right">
                <a class="btn right" href="{{ path('fos_user_security_logout') }}">Se déconnecter</a>
            </p>
            <h4>{{ app.user.ville.nom}}</h4>

            {{ form_start(form) }}
            {{ form_widget(form.demande_id) }}
            {{ form_rest(form) }}
            <input type="submit" value="Télécharger">
            {{ form_end(form) }}

        </section>
    </main>
{% endblock %}
